<Defs>

	<!-- Single Uranium -->


  <RecipeDef>
    <defName>Make_RefinedUranium</defName>
    <label>refine uranium</label>
    <description>Refine uranium.</description>
    <jobString>Refining uranium.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
	<researchPrerequisite>UraniumProcessing</researchPrerequisite>
    <soundWorking>Recipe_Cremate</soundWorking>
    <workAmount>1500</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Uranium</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
		<li>Uranium</li>
		<li>Silver</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <RefinedUranium>1</RefinedUranium>
    </products>
    <skillRequirements>
      <Intellectual>8</Intellectual>
	  <Crafting>5</Crafting>
    </skillRequirements>
    <workSkill>Intellectual</workSkill>
  </RecipeDef>
  
  
